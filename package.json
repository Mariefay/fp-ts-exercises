{
  "type":"module",
  "dependencies": {
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "fp-ts": "^2.16.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitest/ui": "^3.2.2",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "vitest": "^3.2.2"
  },
  "scripts": {
    "exercise": "node scripts/exercise.js",
    "solution": "cross-env SOLUTION=true node scripts/exercise.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src/**/*.ts scripts/**/*.js",
    "lint:fix": "eslint src/**/*.ts scripts/**/*.js --fix",
    "format": "prettier --write src/**/*.ts scripts/**/*.js",
    "format:check": "prettier --check src/**/*.ts scripts/**/*.js",
    "typecheck": "tsc --noEmit"
  }
}
